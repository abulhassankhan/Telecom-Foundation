<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>

        <record id="ah_crm_region_view_tree" model="ir.ui.view">
            <field name="name">ah.crm.region.view.tree</field>
            <field name="model">crm.region</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record id="ah_action_crm_region" model="ir.actions.act_window">
            <field name="name">Region</field>
            <field name="res_model">crm.region</field>
            <field name="view_mode">tree</field>
        </record>

        <menuitem
            id="menu_crm_region"
            name="Region"
            parent="crm.crm_menu_config"
            action="ah_action_crm_region"
        />

    </data>
</odoo>